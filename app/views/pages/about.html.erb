<!-- Breadcrumbs -->
<section class="breadcrumbs-custom-inset">
  <div class="breadcrumbs-custom context-dark bg-overlay-60">
    <div class="container">
      <h2 class="breadcrumbs-custom-title">About</h2>
      <ul class="breadcrumbs-custom-path">
        <li><a href="index.html">Home</a></li>
        <li class="active">About</li>
      </ul>
    </div>
    <div class="box-position" style="background-image: url(assets/fasil.jpg);"></div>
  </div>
</section>
<!-- Why choose us-->
<section class="section section-sm section-first bg-default text-md-left">
  <div class="container">
    <div class="row row-50 align-items-center justify-content-center justify-content-xl-between">
      <div class="col-lg-6 text-center wow fadeInUp"><%= image_tag @company_information.medias, style:"hegiht:400px" %>
      </div>
      <div class="col-lg-6 wow fadeInRight" data-wow-delay=".1s">
        <div class="box-width-lg-470">
          <h3>Why choose Us</h3>
          <!-- Bootstrap tabs-->
          <div class="tabs-custom tabs-horizontal tabs-line tabs-line-big tabs-line-style-2 text-center text-md-left" id="tabs-7">
            <!-- Nav tabs-->
            <ul class="nav nav-tabs">
              <li class="nav-item" role="presentation"><a class="nav-link active" href="#tabs-7-1" data-toggle="tab">Why choose us</a></li>
              <li class="nav-item" role="presentation"><a class="nav-link" href="#tabs-7-2" data-toggle="tab">our vision</a></li>
              <li class="nav-item" role="presentation"><a class="nav-link" href="#tabs-7-3" data-toggle="tab">Our mission</a></li>
            </ul>
            <!-- Tab panes-->
            <div class="tab-content">
              <div class="tab-pane fade show active" id="tabs-7-1">
                <p><%= @company_information.why_choose_us.html_safe %></p>
                <div class="group-md group-middle"><a class="button button-secondary button-pipaluk" href="/contact">Get in Touch</a></div>
              </div>
              <div class="tab-pane fade" id="tabs-7-2">
                <p><%= @company_information.vision.html_safe %></p>
                <div class="group-md group-middle"><a class="button button-secondary button-pipaluk" href="/contact">Get in Touch</a></div>
              </div>
              <div class="tab-pane fade" id="tabs-7-3">
                <p><%= @company_information.mission.html_safe %></p>
                <div class="group-md group-middle"><a class="button button-secondary button-pipaluk" href="/contact">Get in Touch</a></div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>
<!--Counters-->
<!-- Counter Classic-->
<section class="section section-fluid bg-default ">
  <div class="parallax-container" data-parallax-img="assets/Lalibela2.jpg">
    <div class="parallax-content section-xl context-dark bg-overlay-26 parallax" style="background-image:url(<%= asset_path('Lalibela2.jpg') %>);  min-height:50px;">
      <div class="container">
        <div class="row row-50 justify-content-center border-classic">
          <div class="col-sm-6 col-md-5 col-lg-4">
            <div class="counter-classic">
              <div class="counter-classic-number"><span class="counter"><%= @company_information.number_of_destinations%></span>
              </div>
              <h5 class="counter-classic-title">Destinations</h5>
            </div>
          </div>
          <div class="col-sm-6 col-md-5 col-lg-4">
            <div class="counter-classic">
              <div class="counter-classic-number"><span class="counter"><%= @company_information.number_of_tours%></span>
              </div>
              <h5 class="counter-classic-title">Tours</h5>
            </div>
          </div>
          <div class="col-sm-6 col-md-5 col-lg-4">
            <div class="counter-classic">
              <div class="counter-classic-number"><span class="counter"><%= @company_information.number_of_staff_members%></span>
              </div>
              <h5 class="counter-classic-title">Team members</h5>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>
<!-- Latest Projects-->
<section class="section section-sm bg-gray-2">
  <div class="container">
    <h3>Our Services</h3>
    <div class="row row-30">
      <% @services.each do |service|%>
        <div class="col-sm-6 col-lg-4">
          <article class="box-icon-classic">
            <div class="unit box-icon-classic-body flex-column flex-md-row text-md-left flex-lg-column text-lg-center flex-xl-row text-xl-left">
              <div class="unit-left">
                <div class="box-icon-classic-icon fl-bigmug-line-equalization3"></div>
              </div>
              <div class="unit-body">
                <h5 class="box-icon-classic-title"><%= service.service_name.html_safe  %></h5>
                <p class="box-icon-classic-text"><%= service.service_description.truncate(256).html_safe %></p>
              </div>
            </div>
          </article>
        </div>
      <%end%>
    </div>
  </div>
</section>
<!-- What people Say-->
<section class="section section-sm section-last bg-default mb-0">
  <div class="container mb-0">
    <h3>What People Say</h3>
  </div>
</section>
<div id="carouselExampleControls" class="carousel slide carousel-fade " data-ride="carousel" style="max-height:1000px; " data-interval="2000">
  <div class="carousel-inner" style=" height:500px;">
    <%if !@testimonials.nil?%>
      <div class="carousel-item active parallax" style="background-image: url('<%= asset_path("Lalibela2.jpg")%>'); height:80px;" >
        <%# <img class="d-block w-100" src="assets/fasil.jpg" alt="First slide" style="max-height:500px; object-fit: cover;"> %>
        <div class="carousel-caption" data-aos="fade-up" data-aos-delay="400">
          <article class="quote-lisa">
            <div class="quote-lisa-body" >
              <div class="quote-lisa-text" >
                <p class="q text-dark"><%= @testimonials[0].message.html_safe %></p>
              </div>
              <h5 class="quote-lisa-cite"><%= @testimonials[0].full_name.html_safe %></h5>
            </div>
          </article>
        </div>
      </div>
      <% @testimonials.drop(1).each do |t|%>
        <div class="carousel-item parallax" style="background-image: url('<%= asset_path("Lalibela2.jpg")%>'); height:80px;" >
          <%# <img class="d-block w-100" src="assets/fasil.jpg" alt="First slide" style="max-height:500px; object-fit: cover;"> %>
          <div class="carousel-caption" data-aos="fade-up" data-aos-delay="400">
            <article class="quote-lisa">
              <div class="quote-lisa-body" >
                <div class="quote-lisa-text" >
                  <p class="q text-dark"><%= t.message.html_safe %></p>
                </div>
                <h5 class="quote-lisa-cite"><%= t.full_name.html_safe %></h5>
              </div>
            </article>
          </div>
        </div>
      <%end%>
    <%end%>
  </div>
  <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
    <span class="sr-only">Previous</span>
  </a>
  <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
    <span class="carousel-control-next-icon" aria-hidden="true"></span>
    <span class="sr-only">Next</span>
  </a>
</div>
